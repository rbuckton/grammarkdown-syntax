{
  "name": "grammarkdown-language",
  "version": "0.3.2",
  "displayName": "Grammarkdown Language Support",
  "description": "Grammarkdown Language Support for Visual Studio Code",
  "license": "Apache-2.0",
  "keywords": [
    "grammar",
    "language",
    "syntax",
    "dsl",
    "javascript",
    "typescript",
    "ecmascript"
  ],
  "author": {
    "name": "Ron Buckton",
    "email": "rbuckton@chronicles.org",
    "url": "http://github.com/rbuckton"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rbuckton/grammarkdown-client.git"
  },
  "publisher": "rbuckton",
  "private": true,
  "scripts": {
    "build": "gulp build",
    "prepublish": "gulp build",
    "vscode:prepublish": "gulp build"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "grammarkdown",
        "aliases": [
          "Grammarkdown",
          "grammar"
        ],
        "extensions": [
          ".grammar",
          ".gmd"
        ],
        "configuration": "./grammarkdown.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "grammarkdown",
        "scopeName": "source.grammar",
        "path": "./out/syntax/grammarkdown.tmLanguage"
      }
    ]
  },
  "engines": {
    "vscode": "^0.10.1"
  },
  "devDependencies": {
    "@types/node": "^4.0.29",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "js-yaml": "^4.1.0",
    "plist": "^3.0.4"
  }
}
